{
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^2.75.0",
		"axios": "^1.12.2",
		"dotenv": "^17.2.3"
	},
	"scripts": {
		"test": "node --test",
		"supabase:login": "npx supabase login",
		"supabase:init": "npx supabase init --workdir ./supabase",
		"supabase:link": "npx supabase link --workdir ./supabase",
		"supabase:status": "npx supabase migration list --workdir ./supabase",
		"db:push": "npx supabase db push --workdir ./supabase",
		"db:push:dry": "npx supabase db push --dry-run --workdir ./supabase",
		"db:start:local": "npx supabase start --workdir ./supabase",
		"db:stop:local": "npx supabase stop --workdir ./supabase",
		"db:reset:local": "npx supabase db reset --workdir ./supabase",
		"db:diff": "npx supabase db diff --workdir ./supabase",
		"db:query:users": "npx supabase db query --file ./supabase/migrations/001_init-users.sql --workdir ./supabase",
		"db:query:orders": "npx supabase db query --file ./supabase/migrations/002_init-orders.sql --workdir ./supabase"
	},
	"devDependencies": {
		"supabase": "^2.53.6"
	}
}
